<div class="page-header">
  <%= link_to price_schemas_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_price_schema_path(@price_schema), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show price_schema</h1>
</div>

<dl class="dl-horizontal">
  <dt>Quantity:</dt>
  <dd><%= @price_schema.quantity %></dd>

  <dt>Unit:</dt>
  <dd><%= @price_schema.unit %></dd>

  <dt>Price:</dt>
  <dd><%= @price_schema.price %></dd>

</dl>
