<div class="page-header">
  <%= link_to new_photo_product_path, :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Photo product
  <% end %>
  <h1>Listing photo_products</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Name</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @photo_products) do |photo_product| %>
            <td><%= photo_product.name %></td>
            <td><%= link_to 'Show', photo_product %></td>
        <td><%= link_to 'Edit', edit_photo_product_path(photo_product) %></td>
        <td><%= link_to 'Destroy', photo_product, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
